<?xml version="1.0" encoding="UTF-8"?>
<domains>
	<domain>
		<name>User</name>
        <tablename>users</tablename>
		<cnName>用户</cnName>
		<properties>
			<property>
				<name>id</name>
				<cnName>用户id</cnName>
				<type>int</type>
				<empty>false</empty>
				<idx>false</idx>
				<pk>true</pk>
				<ref>false</ref>
			</property>
			<property>
				<name>username</name>
				<cnName>用户名</cnName>
				<type>String</type>
				<empty>false</empty>
				<idx>true</idx>
				<pk>false</pk>
				<ref>false</ref>
			</property>
			<property>
				<name>password</name>
				<cnName>密码</cnName>
				<type>String</type>
				<empty>false</empty>
				<idx>false</idx>
				<pk>false</pk>
				<ref>false</ref>
			</property>
			<property>
				<name>enabled</name>
				<cnName>是否可用</cnName>
				<type>boolean</type>
				<empty>false</empty>
				<idx>false</idx>
				<pk>false</pk>
				<ref>false</ref>
			</property>
		</properties>
	</domain>
	<domain>
        <name>Authority</name>
        <tablename>authorities</tablename>
		<cnName>权限</cnName>
		<properties>
			<property>
				<name>username</name>
				<cnName>用户名</cnName>
				<type>String</type>
				<empty>false</empty>
				<idx>false</idx>
				<pk>false</pk>
				<ref>
					<refClass>User</refClass>
					<refClassProp>username</refClassProp>
					<refType>one-to-one</refType>
				</ref>
			</property>
			<property>
				<name>authority</name>
				<cnName>角色名</cnName>
				<type>String</type>
				<empty>false</empty>
				<idx>false</idx>
				<pk>false</pk>
				<ref>false</ref>
			</property>
		</properties>
	</domain>
	<domain>
		<name>apply</name>
		<cnName>请假申请</cnName>
		<properties>
			<property>
				<name>apply_id</name>
				<name_cn>申请id</name_cn>
				<type>int</type>
				<empty>false</empty>
				<idx>false</idx>
				<pk>true</pk>
				<ref>false</ref>
			</property>
			<property>
				<name>apply_user</name>
				<name_cn>申请用户</name_cn>
				<type>int</type>
				<empty>false</empty>
				<idx>false</idx>
				<pk>false</pk>
				<ref>
					<ref_table>users</ref_table>
					<ref_table_col>user_id</ref_table_col>
					<ref_type>many-to-one</ref_type>
				</ref>
			</property>
			<property>
				<name>apply_start</name>
				<name_cn>开始时间</name_cn>
				<type>Date</type>
				<empty>false</empty>
				<idx>false</idx>
				<pk>false</pk>
				<ref>false</ref>
			</property>
			<property>
				<name>apply_end</name>
				<name_cn>结束时间</name_cn>
				<type>Date</type>
				<empty>false</empty>
				<idx>false</idx>
				<pk>false</pk>
				<ref>false</ref>
			</property>
			<property>
				<name>apply_reason</name>
				<name_cn>申请原因</name_cn>
				<type>String</type>
				<empty>false</empty>
				<idx>false</idx>
				<pk>false</pk>
				<ref>false</ref>
			</property>
		</properties>
	</domain>
</domains>